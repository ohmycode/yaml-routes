{
    "name": "yaml-routes-monorepo",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "concurrently \"pnpm --filter @yaml-routes/tanstack dev\" \"pnpm --filter demo dev\"",
        "build": "pnpm -r build",
        "test": "pnpm -r test",
        "type-check": "pnpm -r type-check",
        "lint": "pnpm -r lint",
        "clean": "pnpm -r clean",
        "demo:dev": "pnpm --filter demo dev",
        "demo:build": "pnpm --filter demo build",
        "package:build": "pnpm --filter @yaml-routes/tanstack build",
        "package:test": "pnpm --filter @yaml-routes/tanstack test",
        "changeset": "changeset",
        "version-packages": "changeset version",
        "release": "pnpm build && changeset publish"
    },
    "devDependencies": {
        "@changesets/cli": "^2.27.1",
        "@types/node": "^22.10.6",
        "concurrently": "^9.2.1",
        "typescript": "^5.7.2"
    },
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    },
    "packageManager": "pnpm@8.15.0"
}
