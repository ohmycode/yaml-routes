{
    "name": "@yaml-routes/tanstack",
    "version": "0.2.5",
    "description": "YAML-based routing configuration for TanStack Router with i18n support",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        },
        "./cli": {
            "import": "./dist/cli.js",
            "types": "./dist/cli.d.ts"
        }
    },
    "bin": {
        "yaml-routes": "./dist/cli.js"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "test": "vitest run",
        "test:watch": "vitest",
        "type-check": "tsc --noEmit",
        "lint": "eslint src",
        "clean": "rm -rf dist"
    },
    "keywords": [
        "tanstack-router",
        "routing",
        "yaml",
        "i18n",
        "typescript",
        "react"
    ],
    "author": "ohmycode",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ohmycode/yaml-routes.git",
        "directory": "packages/tanstack"
    },
    "homepage": "https://ohmycode.github.io/yaml-routes",
    "bugs": {
        "url": "https://github.com/ohmycode/yaml-routes/issues"
    },
    "peerDependencies": {
        "@tanstack/react-router": ">=1.130.0",
        "react": ">=18.0.0",
        "react-dom": ">=18.0.0"
    },
    "dependencies": {
        "js-yaml": "^4.1.0",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^22.10.6",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "@testing-library/dom": "^10.4.0",
        "@testing-library/react": "^16.2.0",
        "@vitejs/plugin-react": "^4.3.4",
        "jsdom": "^26.0.0",
        "tsup": "^8.0.0",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2",
        "vitest": "^3.0.5"
    }
}
